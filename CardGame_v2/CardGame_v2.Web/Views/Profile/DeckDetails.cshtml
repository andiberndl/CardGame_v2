@model IEnumerable<Card>

@{
    ViewBag.Title = "DeckDetails";
}

 
<h2 style="color:lawngreen">DeckDetails</h2>

@Html.ActionLink("Edit Deck", "EditDeck", "DeckBuilder", new { id = ViewContext.RouteData.Values["id"] }, new { @style = "color:lawngreen"})
@Html.ActionLink("Back to Deck Overview", "DeckOverview", null, new { @style = "color:lawngreen" })

<div class="jumbotron row" style="background-color:transparent; height:750px; overflow:auto">
    @Html.Partial("_CardList", Model)
</div>



